    "def gn_get_links(person):\n",
    "    \"\"\"\n",
    "    Input:\n",
    "        name of the person\n",
    "    Description:\n",
    "        run query on google news with person as topic and date_range as timeframe\n",
    "    Output:\n",
    "        list of links to the articles that refer to the topic\n",
    "    \"\"\"\n",
    "    \n",
    "    # config = Config()\n",
    "    # config.browser_user_agent = USER_AGENT\n",
    "#     config.request_timeout = 10\n",
    "    \n",
    "    stories=[]\n",
    "    \n",
    "    gn = GoogleNews(region='US')\n",
    "    gn.set_time_range('03/01/2019','03/01/2021')\n",
    "    search_result = gn.search(person+\"%2B politician\")\n",
    "    \n",
    "    newsitems = gn.results()\n",
    "\n",
    "    for item in newsitems:\n",
    "        try:\n",
    "#             story_items = {}\n",
    "            \n",
    "            story = item['title']\n",
    "            item_date = item['date']\n",
    "            \n",
    "            url = item['link']\n",
    "            article = Article(url, config=config)\n",
    "\n",
    "            article.download()\n",
    "            article.parse()\n",
    "\n",
    "            title = article.title\n",
    "            date = article.publish_date\n",
    "            text = article.text\n",
    "\n",
    "            article.nlp()\n",
    "            keywords = article.keywords\n",
    "            keywords.sort()\n",
    "    \n",
    "            stories.append([article.url,keywords,article.summary])\n",
    "            \n",
    "        except:\n",
    "            pass\n",
    "        \n",
    "    return stories\n"
   ]
