    "def get_sentiment(names: np.array):\n",
    "    \"\"\"\n",
    "    Input:\n",
    "        names: numpy array of names of the congress members\n",
    "    Description:\n",
    "        Take in a numpy array and return a dictionary with names and overall sentiment value, and a dictionary with \n",
    "        names and a list of values of articles.\n",
    "    Output:\n",
    "        Dictionary with names and overall sentiment value, and a dictionary with names and a list of values of articles.\n",
    "    \"\"\"\n",
    "    sentiment = {}\n",
    "    articles = {}\n",
    "    config = Config()\n",
    "    config.browser_user_agent = USER_AGENT\n",
    "    config.request_timeout = 10\n",
    "    for i in range(names.shape[0]):\n",
    "        name = names[i]\n",
    "        print(i,' | ',name)\n",
    "        with open(f'Links2/pol_{i}.txt', 'r') as f:\n",
    "            urls = f.read().split('\\n')\n",
    "        sent = 0\n",
    "        art = 0\n",
    "        for url in urls:\n",
    "            try:\n",
    "                article = Article(url, config=config)\n",
    "\n",
    "                article.download()\n",
    "                article.parse()\n",
    "\n",
    "                title = article.title\n",
    "                date = article.publish_date\n",
    "                text = article.text\n",
    "\n",
    "                article.nlp()\n",
    "                keywords = article.keywords\n",
    "                keywords.sort()\n",
    "                sent += run_sent_analysis(text)\n",
    "        \n",
    "                art.append(article.url,keywords,article.summary, title, date)\n",
    "                \n",
    "            except:\n",
    "                pass\n",
    "        sentiment[name] = sent\n",
    "        \n",
    "    return sentiment, articles"
   ]
  },
