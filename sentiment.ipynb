    "def save_links(names, count):\n",
    "    \"\"\"\n",
    "    Input:\n",
    "        names: list of names of the congress members\n",
    "        count: number of the chunk of the dataframe\n",
    "    Description:\n",
    "        Saves the links to the articles that refer to the congress members\n",
    "    Output:\n",
    "        txt file with the links in Links2 folder\n",
    "    \"\"\"\n",
    "    for name in names:\n",
    "        goog = GoogleNews(lang='en', region='US')\n",
    "        goog.set_time_range('03/01/2019','03/01/2021')\n",
    "        goog.search(name+\"%2B politician\")\n",
    "        links = goog.get_links()\n",
    "        filepath = f'Links2/pol_{count}.txt'.format(count=count)\n",
    "        try:\n",
    "            with open(filepath, 'w') as f:\n",
    "                f.write('\\n'.join(links))\n",
    "            print(r'{count} | {name} has been saved succesfully'.format(name=name, count=count))\n",
    "        except:\n",
    "            print(r'{count} | {name} HAD MISTAKES AAAHHHH'.format(name=name, count=count))\n",
    "            pass\n",
    "        count += 1"
   ]
