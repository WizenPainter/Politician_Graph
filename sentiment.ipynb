    "def get_links(name: str):\n",
    "    gn = GoogleNews(region='US')\n",
    "    gn.set_time_range('03/01/2019','03/01/2021')\n",
    "    gn.search(name+\"%2B politician\")\n",
    "\n",
    "    config = Config()\n",
    "    config.browser_user_agent = USER_AGENT\n",
    "    config.request_timeout = 10\n",
    "    \n",
    "    links = gn.get_links()\n",
    "    print(name+' Download Begin')\n",
    "    stories = [] \n",
    "    for url in links:\n",
    "        article = Article(url, config=config)\n",
    "        try:\n",
    "            article.download()\n",
    "            article.parse()\n",
    "\n",
    "            title = article.title\n",
    "            date = article.publish_date\n",
    "            text = article.text\n",
    "\n",
    "            article.nlp()\n",
    "            keywords = article.keywords\n",
    "            keywords.sort()\n",
    "            \n",
    "\n",
    "            stories.append([article.url,keywords,article.summary])\n",
    "        except:\n",
    "            pass\n",
    "\n",
    "        \n",
    "    print(name+' Download end')\n",
    "    return stories"
   ]
